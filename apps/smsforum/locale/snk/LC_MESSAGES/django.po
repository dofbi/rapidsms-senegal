#: app.py:248
msgid "leave-success %(villages)s"
msgstr "An bagu dingiran wa '%(villages)s'. Ma ta tana!"
#msgstr "Vous venez de quitter la communauté '%(villages)s'. Au revoir!"

msgid "leave-fail_village-not-found %(village)s"
msgstr "N kori ke dingira walla."
#msgstr "Je ne peux pas trouver la communauté qui s'appelle '%(village)s'"

